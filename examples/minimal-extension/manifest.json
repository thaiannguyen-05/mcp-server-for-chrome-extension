{
    "manifest_version": 3,
    "name": "MCP Extension Example",
    "version": "1.0.0",
    "description": "Example Chrome extension using MCP Extension Library",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "src/background/sw.js",
        "type": "module"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}